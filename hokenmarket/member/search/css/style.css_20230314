#selectTbl {
	width: 100%;
	border: 1px solid #e9e8e8;
	border-collapse: collapse;
	border-spacing: 0;
}
#selectTbl th {
	background: #f6f4f4;
	border: #e9e8e8 solid;
	border-width: 0 0 1px 1px;
	font-weight: bold;
	font-size: 18px;
	padding: 15px 25px;
	text-align: left;
	line-height: 1.3;
	vertical-align: middle;
	width: 140px;
}
#selectTbl td input {
	margin: 0 10px 0 0;
}
#selectTbl label {
	display: block;
	text-align: left;
}
#selectTbl label:after {
	content: "";
	clear: both;
	display: block;
}
#selectTbl label input { float: left; }
#selectTbl label p {
	float: left;
	width: calc(70% - 24px);
	margin: -5px 0 0 0;
}
#selectTbl tr th,
#selectTbl tr td {
	box-sizing: border-box;
	display: block;
	width: 100%;
}

#selectTbl td ul{
	width: 1098px;
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	flex-wrap: wrap;
	-webkit-flex-wrap: wrap;
	-ms-flex-wrap:wrap

}
#selectTbl td ul li {
	width: 25%;
	padding: 22px 2% 0 2%;
}
#selectTbl td ul li.col1 {
	width: 100%;
}
#selectTbl td ul li.col2 {
	width: 50%;
}
#selectTbl td ul li.col3 {
	width: 33%;
}
#selectTbl span {
    display: inline;
}
